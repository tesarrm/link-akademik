 "html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Format Data</title>\r\n    <link rel=\"stylesheet\" href=\"styles.css\">\r\n</head>\r\n<body>\r\n    <div class=\"container\">\r\n        <h2>DATA SARANA PRASARANA </h2>\r\n        <!--<div class=\"info\">-->\r\n        <!--    <p>Nama Sekolah : .............................................................</p>-->\r\n        <!--    <p>Alamat : .............................................................</p>-->\r\n        <!--    <p>Nama Kepala Sekolah : .............................................................</p>-->\r\n        <!--    <p>No. HP : .............................................................</p>-->\r\n        <!--</div>-->\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th>Data</th>\r\n                    <th>Hasil</th>\r\n                    <!--<th>Satuan</th>-->\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr>\r\n                    <td>1. Tanah</td>\r\n                    <td>{{ doc.nama }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;a. Kepemilikan</td>\r\n                    <td>{{ doc.kepemilikan }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;b. No Sertifikat</td>\r\n                    <td>{{ doc.no_sertifikat }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;c. Panjang Tanah (m)</td>\r\n                    <td>{{ doc.panjang_m }}</td>\r\n                    <!--<td>m<sup>2</sup></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;d. Lebar Tanah (m)</td>\r\n                    <td>{{ doc.lebar_m }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;e. Luas Tanah</td>\r\n                    <td>{{ doc.luas_m2 }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;f. Luas Lahan Tersedia</td>\r\n                    <td>{{ doc.luas_lahan_tersedia_m2 }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                 <tr>\r\n                    <td>&emsp;g. Ket Tanah</td>\r\n                    <td>{{ doc.ket_tanah }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                 <tr>\r\n                    <td>&emsp;h. Ket Tanah</td>\r\n                    <td>{{ doc.ket_tanah }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;i. Lokasi Tanah</td>\r\n                    <td>{{ doc.nama_dusun }} - {{ doc.desa_kelurahan }} - {{ doc.kecamatan }} - RT {{ doc.rt }} - RW {{ doc.rw }} - {{ doc.alamat_jalan }}</td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;j. Bangunan</td>\r\n                    <td>\r\n                        {{frappe.db.get_list(doctype=\"Bangunan\", filters={\"nama\": doc.bangunan}, fields=['name', 'nama'])[0].nama}}\r\n                    </td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>2. Bangunan</td>\r\n                    <td></td>\r\n                    <!--<td></td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;a. Jumlah Bangunan</td>\r\n                    <td></td>\r\n                    <!--<td>Unit</td>-->\r\n                </tr>\r\n                <tr>\r\n                    <td>&emsp;b. Luas Bangunan</td>\r\n                    <td></td>\r\n                    <!-