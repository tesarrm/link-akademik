  "html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n  <meta charset=\"UTF-8\">\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n\r\n</head>\r\n<body>\r\n  <div class=\"profil-container\">\r\n    <div class=\"left-column\">\r\n      <form>\r\n        <div class=\"form-group\">\r\n          <label for=\"username\">Username:</label>\r\n          <input type=\"text\" id=\"username\" name=\"username\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"nama\">Nama:</label>\r\n          <input type=\"text\" id=\"nama\" name=\"nama\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"no_hp\">No HP:</label>\r\n          <input type=\"text\" id=\"no_hp\" name=\"no_hp\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"user_telegram\">User Telegram:</label>\r\n          <input type=\"text\" id=\"user_telegram\" name=\"user_telegram\">\r\n        </div>\r\n        <input type=\"button\" value=\"Simpan\" onclick=\"saveProfile()\">\r\n      </form>\r\n    </div>\r\n    <div class=\"right-column\">\r\n      <div class=\"profile-pic\">\r\n        <img id=\"profile_image\" src=\"\" alt=\"Gambar Profil\">\r\n        <input type=\"file\" id=\"profile_image_input\" style=\"display: none;\" onchange=\"displayImage('profile_image', this)\">\r\n        <input type=\"button\" value=\"Ubah Gambar Profil\" onclick=\"document.getElementById('profile_image_input').click()\">\r\n      </div>\r\n      <div class=\"school-pic\">\r\n        <img id=\"school_image\" src=\"\" alt=\"Gambar Sekolah\">\r\n        <input type=\"file\" id=\"school_image_input\" style=\"display: none;\" onchange=\"displayImage('school_image', this)\">\r\n        <input type=\"button\" value=\"Ubah Gambar Sekolah\" onclick=\"document.getElementById('school_image_input').click()\">\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n \r\n</body>\r\n</html>\r\n",
  "modified": "2024-07-04 15:30:34.999320",
  "name": "ProfilPengguna",
  "private": 0,
  "roles": [],
  "script": "\r\n    function saveProfile() {\r\n      var username = document.getElementById(\"username\").value;\r\n      var nama = document.getElementById(\"nama\").value;\r\n      var no_hp = document.getElementById(\"no_hp\").value;\r\n      var user_telegram = document.getElementById(\"user_telegram\").value;\r\n\r\n      // Lakukan aksi penyimpanan data di sini\r\n      console.log(\"Data disimpan:\", { username, nama, no_hp, user_telegram });\r\n    }\r\n\r\n    function displayImage(imageId, input) {\r\n      var file = input.files[0];\r\n      var reader = new FileReader();\r\n      reader.onload = function(e) {\r\n        document.getElementById(imageId).src = e.target.result;\r\n        // Lakukan aksi upload gambar di sini\r\n        console.log(\"Gambar diunggah:\", file);\r\n      };\r\n      reader.readAsDataURL(file);\r\n    }\r\n",
  "style": "    body {\r\n      font-family: Arial, sans-serif;\r\n      background-color: #f0f0f0;\r\n      margin: 0;\r\n      padding: 0;\r\n    }\r\n\r\n    .profil-container {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      max-width: 800px;\r\n      margin: 20px auto;\r\n      background-color: #fff;\r\n      border-radius: 8px;\r\n      box-shadow: 0 0 10px rgba(0,0,0,0.1);\r\n      padding: 20px;\r\n    }\r\n\r\n    .left-column, .right-column {\r\n      width: 48%;\r\n      padding: 0 10px;\r\n    }\r\n\r\n    .form-group {\r\n      margin-bottom: 20px;\r\n    }\r\n\r\n    .form-group label {\r\n      display: block;\r\n      margin-bottom: 8px;\r\n      font-weight: bold;\r\n    }\r\n\r\n    .form-group input[type=\"text\"] {\r\n      width: calc(100% - 16px);\r\n      padding: 10px;\r\n      border: 1px solid #ccc;\r\n      border-radius: 4px;\r\n      font-size: 16px;\r\n    }\r\n\r\n    input[type=\"button\"] {\r\n      background-color: #4CAF50;\r\n      color: white;\r\n      border: none;\r\n      padding: 12px 20px;\r\n      text-align: center;\r\n      text-decoration: none;\r\n      display: inline-block;\r\n      font-size: 16px;\r\n      border-radius: 4px;\r\n      cursor: pointer;\r\n    }\r\n\r\n    input[type=\"button\"]:hover {\r\n      background-color: #45a049;\r\n    }\r\n\r\n    .profile-pic img, .school-pic img {\r\n      width: 100%;\r\n      max-width: 150px;\r\n      height: auto;\r\n      border-radius: 50%;\r\n      border: 3px solid #fff;\r\n      box-shadow: 0 0 10px rgba(0,0,0,0.1);\r\n    }\r\n\r\n    .profile-pic input, .school-pic input {\r\n      margin-top: 10px;\r\n    }"
 },
